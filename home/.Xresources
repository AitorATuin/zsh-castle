URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.perl-ext-common: default,matcher,tabbedex,keyboard-select,resize-font,clipboard
URxvt.clipboard.autocopy: true
URxvt.tabbed.autohide: true
URXvt.tabbed.reopen-on-close: yes
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.Shift-Up: perl:tabbedex:new_tab
URxvt.keysym.Shift-Right: perl:tabbedex:next_tab
URxvt.keysym.Shift-Left: perl:tabbedex:prev_tab
URxvt.keysym.Control-Shift-Left: perl:tabbedex:move_tab_left
URxvt.keysym.Control-Shift-Right: perl:tabbedex:move_tab_right
URxvt.keysym.Shift-Down: perl:tabbedex:rename_tab
URxvt.keysym.M-Down: \033[1;3B
URxvt.keysym.M-Up: \033[1;3A
URxvt.keysym.M-Left: \033[1;3D
URxvt.keysym.M-Right: \033[1;3C
URxvt.tabbed.no-tabbedex-keys: true
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.scrollBar: false
URxvt.cursorBlink:true
URxvt.letterSpace: 0
URxvt.lineSpace: 2
URxvt.tripleclickwords: true
URxvt.insecure: true
! |- Definimos el tipo de fuente a usar (para obtener una lista completa de fuentes disponibles en su sistema use ¿fc-list¿ en su consola).
!URxvt*font: xft:Ubuntu Mono derivative Powerline
!URxvt*font: xft:Inconsolata for Powerline:pixelsize=16
! |- Si se presenta separación entre caracteres agregue, donde -1 es la cantidad de pixeles a disminuir
! |- Definimos el color de fondo de las pestañas
! |- Definimos el color frontal de las pestañas
! |- Definimos el color de fondo de los separadores de pestañas
! |- Definimos el color frontal de los separadores de las pestañas
URxvt.font: xft:Cousine for Powerline
URxvt.boldFont: xft:Cousine for Powerline
!URxvt.font: xft:Inconsolata
!URxvt.boldFont: xft:Inconsolata
URxvt*meta8: false

URxvt.cursorColor: #83C048
URxvt.highlightColor: #444444
URxvt.underlineColor: #999999
URxvt.colorUL: #4682B4

